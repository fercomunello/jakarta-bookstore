# => Run configuration script via ./jboss-cli.sh

# Start the server in admin-only mode
embed-server --std-out=echo

# Show runtime info
version

# Set production profile
/system-property=profile:add(value=production)

/subsystem=undertow/servlet-container=default:write-attribute(name=allow-non-standard-wrappers,value=true)

# Stop the embedded server
stop-embedded-server